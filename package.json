{
  "name": "react-redux-template",
  "version": "3.1.0",
  "description": "React redux starter kit",
  "main": "server.js",
  "private": true,
  "scripts": {
    "start": "node server",
    "dist": "webpack --env.production",
    "dist:test": "webpack --env.test",
    "start:dist": "npm run start -- --production",
    "lint:js": "eslint src --ext .js --ext .jsx",
    "lint:js:fix": "eslint src --ext .js --ext .jsx --fix",
    "lint:css": "stylelint src/**/*.css",
    "lint": "npm run lint:js && npm run lint:css",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test:cov": "npm test -- --coverage",
    "precommit": "lint-staged",
    "pretty":
      "prettier-eslint --single-quote --trailing-comma all --write 'src/**/*.js'"
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --trailing-comma all"
    ]
  },
  "author": "Steven Van Eenoo",
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "clean-webpack-plugin": "^0.1.17",
    "compression": "^1.7.1",
    "compression-webpack-plugin": "^1.0.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "eslint": "^4.8.0",
    "eslint-config-dlw": "^2.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.4.0",
    "express": "^4.16.1",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.4",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "json-loader": "^0.5.7",
    "lint-staged": "^4.2.3",
    "lodash-webpack-plugin": "^0.11.4",
    "morgan": "^1.9.0",
    "normalize.css": "^7.0.0",
    "object-assign": "^4.1.1",
    "postcss-loader": "^2.0.6",
    "postcss-reporter": "^5.0.0",
    "prettier": "^1.7.4",
    "prettier-eslint-cli": "^4.4.0",
    "promise": "^8.0.1",
    "proxy-middleware": "^0.15.0",
    "react-hot-loader": "3.0.0-beta.6",
    "redux-logger": "^3.0.6",
    "serve-favicon": "^2.4.5",
    "style-loader": "^0.19.0",
    "stylelint": "^8.1.1",
    "stylelint-config-standard": "^17.0.0",
    "svg-loader": "^0.0.2",
    "url-loader": "^0.6.1",
    "webpack": "^3.6.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.19.1",
    "webpack-merge": "^0.17.0",
    "whatwg-fetch": "^2.0.2",
    "yargs": "^9.0.1"
  },
  "dependencies": {
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*-actions.js",
      "src/**/*-reducers.js",
      "src/**/*-selectors.js"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": ["dlw", "dlw/rules/react"],
    "rules": {},
    "env": {
      "jest": true
    },
    "globals": {
      "__ENV__": true
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard"
  }
}
